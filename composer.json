{
    "name": "inspirum/coding-standard",
    "description": "Inspirum coding standard for PHPCS",
    "keywords": [
        "inspirum",
        "coding standard",
        "phpcodesniffer",
        "phpcs"
    ],
    "homepage": "https://github.com/inspirum/coding-standard-php",
    "license": "MIT",
    "type": "phpcodesniffer-standard",
    "authors": [
        {
            "name": "Tomáš Novotný",
            "email": "tomas.novotny@inspirum.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "doctrine/coding-standard": "^11.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
